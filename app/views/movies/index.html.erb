<h1>Rotten Mangoes</h1>
<div class="row">
<div class="col-md-4">
<h3><%= link_to "Submit a movie!", new_movie_path %></h3>
</div>
<div class="col-md-4">
<%= render 'search_form' %>
</div>
<div class="col-md-3">
</div>
</div>

<%= paginate @movies %>

<hr>
<% if @movies.present? %>
   <%= render partial: 'movie', collection: @movies %>
<% else %>
  <p>Your search returned no results<%= params[:search] %>.</p>
<% end %>
  



